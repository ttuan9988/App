<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TrangAdmin.Dinhvi.guidvxe">

    <ListView
        android:id="@+id/listdsbat"
        android:layout_width="379dp"
        android:layout_height="300dp"
        android:layout_marginTop="92dp"
        android:background="@drawable/vien"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="356dp"
        android:layout_height="45dp"
        android:layout_marginTop="56dp"
        android:onClick="guivitri"
        android:text="Bật vị trí"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtguivt" />

    <TextView
        android:id="@+id/txtguivt"
        android:layout_width="336dp"
        android:layout_height="39dp"
        android:layout_marginTop="56dp"
        android:text=""
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.453"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listdsbat" />
</androidx.constraintlayout.widget.ConstraintLayout>